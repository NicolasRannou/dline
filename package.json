    "build": "polymer build --verbose --css-minify --html-minify --bundle --add-service-worker --add-push-manifest --insert-prefetch-links",
    "lint": "npm run lint:javascript",
    "lint:javascript": "eslint . --ext js,html --ignore-path .gitignore",
    "dev": "polymer serve | npm run watch",
    "test": "polymer test",
    "watch": "browser-sync start --proxy localhost:8080 --files \"src/**/*.*, index.html, *.js\"",
    "test:integration": "polymer build # test that psk builds without error with the CLI",
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'bower_components', 'junction') } catch (e) { }\""
  },
  "description": "polymer build --verbose --preset es5-bundled",
